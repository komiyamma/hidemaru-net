#zero_code = '0';

#n = 1;
while (#n < 10) {
    if( iskeydown(#zero_code+#n) ) {
        break;
    }
    #n = #n+1;
}
if (#n == 10) {
    if( !iskeydown(#zero_code) ) {
        #n = 0;
    }
}
if( #n > 0 ){
    setactivehidemaru gettabhandle(0, tabgroup, #n-1);
}
