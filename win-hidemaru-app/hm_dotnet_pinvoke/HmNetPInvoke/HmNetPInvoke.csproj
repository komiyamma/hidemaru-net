<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net47;netcoreapp3.1;net5.0</TargetFrameworks>
    <AssemblyName>HmNetPInvoke</AssemblyName>
    <RootNamespace>Hidemaru</RootNamespace>
    <Version>1.0.0.1</Version>
    <Copyright>Copyright Â© 2021 Akitsugu Komiyama</Copyright>
    <NeutralLanguage>ja-JP</NeutralLanguage>
    <PackageId>HmNetPInvoke</PackageId>
    <Authors>HmNetPInvoke</Authors>
    <Company>HmNetPInvoke</Company>
    <Product>HmNetPInvoke</Product>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <DefineConstants>TRACE;BUILD_DLL</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;BUILD_DLL</DefineConstants>
  </PropertyGroup>

</Project>
